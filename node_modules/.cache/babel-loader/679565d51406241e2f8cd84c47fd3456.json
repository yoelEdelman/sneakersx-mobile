                })\n            },\n            presentAlert(msg) {\n                return this.$ionic.alertController\n                    .create({\n                        header: 'Alert',\n                        message: msg,\n                        buttons: ['OK'],\n                    })\n                    .then(a => a.present())\n            },\n\n        },\n\n\n    }\n</script>"],"sourceRoot":"src/components"}]}